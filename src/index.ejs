<html>
  <head>
    <meta charset="utf-8">
		<meta name="viewport" content="width=device-width,initial-scale=1,minimum-scale=1,maximum-scale=1,user-scalable=no">
    <!-- <title>res</title> -->
		<script src='static/libs/laya.core.js'></script>
		<script src='static/libs/laya.webgl.js'></script>
		<script src='static/libs/laya.filter.js'></script>
		<script src='static/libs/laya.particle.js'></script>
